{
  "compilerOptions": {
    "paths": {
      "dotenv": [
        "./node_modules/dotenv"
      ]
    },
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}